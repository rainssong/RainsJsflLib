var file = fl.configURI + 'Javascript/getSelectedFrames.jsfl';fl.runScript(file); var timeLine = fl.getDocumentDOM().getTimeline()var frameObjectArr = getSelectedFrames(timeLine);InsertLabel(frameObjectArr)function InsertLabel(_frameObjectArr){	var con = confirm("OK:SequenceNumber,Cancel:each")	if(con){		addSelectedFrameLabelSequenceNumber(_frameObjectArr)	}else{		addSelectedFrameLabelName(_frameObjectArr)	}}//選択しているフレームに連番で名前をつけるfunction addSelectedFrameLabelSequenceNumber(_frameObjectArr){	var framsArr = []	for(var i=0;i<_frameObjectArr.length;i++){		var __target = _frameObjectArr[i]		for(var j=0;j<__target.frames.length;j++){			framsArr.push(__target.frames[j])		}	}	var appStr = prompt("Enter prepend to Label","_") || "_";	var startNum =  Number(prompt("Enter StartNum : ",0)) || 0	for(var k=0;k<framsArr.length;k++){		var targetFrame = framsArr[k]		targetFrame.name = appStr+(startNum+k)	}}//選択しているフレームにラベルをつけるfunction addSelectedFrameLabelName(frameObjectArr){	for(var i=0;i<frameObjectArr.length;i++){		var __target = frameObjectArr[i]		addLabel(__target)	}}//ラベルをつけるプロンプトを表示するfunction addLabel(targetObject){	for(var i=0;i<targetObject.frames.length;i++){		var targetFrame =targetObject.frames[i];		var targetFrameIndex = targetFrame.startFrame;		timeLine.currentFrame = targetFrameIndex		var labelName = prompt("Enter Label : [ Layer : "+targetObject.layer.name+"  ] [ Frame : "+(targetFrameIndex+1)+" ]",targetFrame.name || "");		targetFrame.name = labelName	}}