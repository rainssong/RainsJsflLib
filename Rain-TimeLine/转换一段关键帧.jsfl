var timeline = fl.getDocumentDOM().getTimeline();var step = Number(prompt("キーフレームを挿入するステップを指定",5));var arr = timeline.getSelectedFrames();for(var i=0;i<arr.length;i+=3){	var layerIndex = arr[i];	var layer = timeline.layers[layerIndex];	var frameStart = arr[i+1] || step;	timeline.setSelectedLayers(layerIndex);	timeline.setSelectedFrames(frameStart,frameStart);	for(var l=frameStart;l<layer.frameCount;l+=step){		if(frameStart != l)timeline.setSelectedFrames(l,l,false);	}	timeline.convertToKeyframes();}