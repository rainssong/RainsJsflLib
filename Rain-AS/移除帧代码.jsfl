var doc = fl.getDocumentDOM()var lib = doc.library;document.timelines.forEach(function(timeline){	removeScript(timeline)})lib.items.forEach(function(item){	removeScript(item)})function removeScript(item){	if(item instanceof Timeline || (item instanceof SymbolItem && item.timeline)){		var timeline = item.timeline || item		timeline.layers.forEach(function(layer){			layer.frames.forEach(function(frame){				frame.actionScript = "";			})		})	}}