<?xml version="1.0" encoding ="utf-8"?><dialog id="ScaleTimeline" title="TimelineScale" buttons="accept, cancel">	<grid>		<columns>			<column />		</columns>		<rows>			<row>				<radiogroup id="ID_pop1" label="Select Operation:" groupbox="true">					<hbox>						<radio label="scale the timeline:" />						<radio label="delete frames without keys:" />					</hbox>					<hbox>						<label value="     scale-factor" />						<textbox id="ID_scale" value="2" size="6" />						<label value="                delete %" />						<textbox id="ID_delete" value="50" size="6" />					</hbox>				</radiogroup>			</row>			<row>				<radiogroup id="ID_pop2" label="Apply to range:" groupbox="true">					<radio label="all frames" />					<radio label="within from .. to:" />					<hbox>						<label value="      from frame No." />						<textbox id="ID_from" value="1" size="6" />					</hbox>					<hbox>						<label value="      to frame No.    " />						<textbox id="ID_to" value="9999" size="6" />					</hbox>				</radiogroup>			</row>		</rows>	</grid></dialog>