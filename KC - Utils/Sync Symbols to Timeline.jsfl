//Sync Symbols to Timeline//Justin Putney//http://ajarproductions.com//v 0.5.0//This script searches for selected frames containing//graphic symbols and sets their startFrame property//to the current frame in the timeline//Future features ideas://popup menu to select loop setting to: keep current setting, or set to: loop, play once, single frame//checkbox to convert MCs to graphic symbols//check to see if timeline duration is greater than the symbol's timelinevar dom = fl.getDocumentDOM(); var tl = dom.getTimeline(); var frameSeq = tl.getSelectedFrames(); run();function run() {	for(var i=0; i<frameSeq.length; i+=3) {		runOnFrames(frameSeq[i], frameSeq[i+1], frameSeq[i+2]);	}}function runOnFrames(layer, startFrame, endFrame) {	var cLayer = tl.layers[layer];	for(var i=startFrame; i<endFrame; i++){		var cFrame = cLayer.frames[i];		if(cFrame.startFrame == i) {			setSymbolsToFrame(cFrame, i);		} else {			i = cFrame.startFrame + cFrame.duration - 1;			continue;		}	}}function setSymbolsToFrame(frameObj, targFrame) {	for(var i=0; i<frameObj.elements.length; i++){		var cElem = frameObj.elements[i];		if(cElem == "[object SymbolInstance]"){			if(cElem.symbolType == "graphic"){				cElem.firstFrame = targFrame;			}		}	}}