//Set multiple instance names//Justin Putney//http://ajarproductions.com//version 1.0.1var dom = fl.getDocumentDOM(); var tl = dom.getTimeline(); var frameSeq = tl.getSelectedFrames(); getName();function getName(suggestion){	if(suggestion == undefined) suggestion = "";	var iName = prompt("Instance name: ", suggestion);	if(iName != null && iName.length > 0){		if(iName.indexOf(" ") == -1) {			run(iName);		} else {			iName = iName.split(" ").join("_");			alert("An instance name cannot have spaces.");			getName(iName);		}	}}function run(iName){	var count = 0;	if(frameSeq.length < 1) {		alert("You must make a frame selection first.");		return;	}	for(var i=0; i < frameSeq.length; i+=3) {		var layerNum = frameSeq[i];		var startF = frameSeq[(i + 1)];		var endF = frameSeq[(i + 2)];		//loop througn keyframes		for(var j=startF; j < endF; j++){			try {				var iFrame = tl.layers[layerNum].frames[j];				if(j == startF){					j = iFrame.startFrame; //in case 1st selected frame is not a keyframe				}				if(iFrame.startFrame == j) { //this is a keyframe 					count += setElementNames(iFrame, iName);					j += iFrame.duration - 1;					continue;				} else {					j = iFrame.startFrame + iFrame.duration - 1;					continue;				}			} catch(e){}; //skip empty frames		}	}	if(count == 0) {		alert("No movieclips were found in your frame selection.");	} else {		alert(count + " instances have been given the name '" + iName + "'");	}}function setElementNames(frameObj, instanceName) {	var count = 0;	for (var i=0; i< frameObj.elements.length; i++) {		var iSel = frameObj.elements[i];		if(iSel == '[object SymbolInstance]') {			if(iSel.symbolType == 'movie clip'){ 				iSel.name = instanceName;				count++;			}		}	}	return count;}